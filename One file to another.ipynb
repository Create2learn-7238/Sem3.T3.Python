   "source": [
    "### WAP to copy text from one file to another"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "o=open(\"One.txt\",\"r\")\n",
    "a=open(\"Another.txt\",\"w\")\n",
    "a.write(o.read())\n",
    "a.close()\n",
    "o.close()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "with open(\"One.txt\",\"r\") as o:\n",
    "    with open(\"capital.txt\",\"w\") as c:\n",
    "        c.write(o.read().upper())"
   ]
